        "if b == \"+\":\n",
        "  print(a,\"+\", c,\"=\",a+c)\n",
        "elif b == \"-\":\n",
        "   print(a,\"-\",c,\"=\",a-c)\n",
        "elif b == \"*\":\n",
        "  print(a,\"*\",c,\"=\",a*c)\n",
